{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env ENV=dev next dev --port=3001",
    "lint": "cross-env ENV=dev next lint",
    "stage:build": "rm -rf out && cross-env ENV=stage  ANALYZE=false  next build",
    "stage:start": "cross-env ENV=stage  npx serve@latest out",
    "stage-server:start": "cross-env ENV=stage next start",
    "prod:build": "rm -rf out &&  cross-env ENV=prod next build",
    "prod:postbuild": "FRONTEND_URL='https://www.salesjobverse.com.com/' next-sitemap",
    "prod:start": "cross-env ENV=prod npx serve@latest out",
    "stage:deploy": "pnpm run stage:build && aws s3 sync out s3://jobverse-sales-stage && aws cloudfront create-invalidation --distribution-id E3I3YDE6J3PKE --paths '/*'",
    "prod:deploy": "pnpm run prod:build  && aws s3 sync out s3://jobverse-sales-prod && aws cloudfront create-invalidation --distribution-id E310WGTWXO60S8 --paths '/*'"
  },
  "dependencies": {
    "@mantine/core": "^7.1.5",
    "@mantine/dates": "^7.2.2",
    "@mantine/hooks": "^7.1.5",
    "@mantine/modals": "^7.1.5",
    "@mantine/notifications": "^7.1.5",
    "@mantine/tiptap": "^7.5.2",
    "@next/bundle-analyzer": "^14.0.4",
    "@tabler/icons-react": "^2.39.0",
    "@tanstack/react-query": "^4.36.1",
    "@tiptap/extension-highlight": "^2.2.2",
    "@tiptap/extension-link": "^2.2.2",
    "@tiptap/extension-subscript": "^2.2.2",
    "@tiptap/extension-superscript": "^2.2.2",
    "@tiptap/extension-text-align": "^2.2.2",
    "@tiptap/extension-underline": "^2.2.2",
    "@tiptap/pm": "^2.2.2",
    "@tiptap/react": "^2.2.2",
    "@tiptap/starter-kit": "^2.2.2",
    "@ts-rest/core": "^3.30.4",
    "@ts-rest/react-query": "^3.30.4",
    "@types/multer": "^1.4.8",
    "contract": "workspace:1.0.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "embla-carousel-react": "8.0.0-rc15",
    "js-cookie": "^3.0.5",
    "js-file-download": "^0.4.12",
    "lodash": "^4.17.21",
    "next": "13.5.4",
    "next-sitemap": "^4.2.3",
    "pino": "^8.17.2",
    "pino-transmit-http": "^1.0.1",
    "posthog-js": "^1.96.1",
    "react": "^18",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18",
    "react-hook-form": "^7.47.0",
    "react-select": "^5.7.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.5",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "typescript": "^5.2.2"
  }
}
